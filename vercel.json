{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers", 
          "value": "Content-Type, Authorization, X-Requested-With, x-authorization, x-auth-token, x-supabase-token, X-Signature-SHA256, X-Delivery-Id"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path((?!api|wise-webhook).*)",
      "has": [
        {
          "type": "host",
          "value": "soundbridge.live"
        }
      ],
      "destination": "https://www.soundbridge.live/:path*",
      "permanent": true
    }
  ],
  "functions": {
    "apps/web/app/api/upload/fingerprint/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "crons": [
    {
      "path": "/api/cron/downgrade-past-due",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/moderate-content",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/process-pending-notifications",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/expire-grace-periods",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/process-account-deletions",
      "schedule": "0 2 * * *"
    }
  ]
}
